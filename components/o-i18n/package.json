    "start-prod": "stencil build --prod --watch --serve",
    "test": "jest",
    "test:ci": "jest --ci",
    "test.watch": "jest --watch",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@stencil/core": "0.15.0-0",
    "@stencil/postcss": "^0.1.0",
    "@stencil/sass": "0.1.1",
    "@types/i18next": "^11.9.3",
    "@types/jest": "^23.3.1",
    "autoprefixer": "^9.0.2",
    "jest": "^23.4.2",
    "workbox-build": "3.4.1"
  },
  "author": "@o-rango",
  "license": "MIT",
  "homepage": "https://o-rango.github.io/",
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "gitHead": "2936be651042bb20b998bc8bf40418799b604317",
  "dependencies": {
    "@o-rango/o-button": "^0.2.2",
