{
  "name": "@o-rango/o-i18n",
  "version": "0.0.1",
  "description": "@o-rango/o-i18n",
  "main": "dist/o-i18n.js",
  "types": "dist/o-i18n.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
